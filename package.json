{
  "name": "musik-plattform-backend",
  "version": "1.0.0",
  "description": "Sicheres Backend fÃ¼r die Musik-Plattform mit API-Proxy",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "security-scan": "node security-monitor.js",
    "security-report": "node security-monitor.js > logs/security-output.log",
    "test": "npm run test:unit && npm run test:integration && npm run test:security",
    "test:unit": "jest --testPathPattern=tests/unit",
