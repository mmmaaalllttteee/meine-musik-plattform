    "lint:fix": "eslint . --fix",
    "audit": "npm audit --audit-level moderate",
    "audit:fix": "npm audit fix"
  },
  "keywords": [
    "musik",
    "backend",
    "api",
    "proxy",
    "firebase",
    "gemini"
